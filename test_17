    <?php


    $deportes = array("nike", "adidas", "puma", "reebok", "vans", "decathlon");

    $firstElement = $deportes[0];
    echo "The first element of the array is $firstElement.";
    echo "<br>";

    $deportes2 = array("nike", "adidas", "puma", "reebok", "vans", "decathlon");

    $lastElement = $deportes2[count($deportes2) - 1];
    echo "The last element of the array is $lastElement.";

    $result = array_intersect($deportes, $deportes2);
    print_r($result);


    ?>


